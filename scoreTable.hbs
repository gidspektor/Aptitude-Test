{{#each data}}
<div class="entry scoreEntry" data-Id="{{this.id}}" data-Name="{{this.name}}" data-Email="{{this.email}}">
    <div class="score_item">
        <p class="name">Name: {{this.name}}</p>
        <p class="email">Email: {{this.email}}</p>
        <p class="score">Score: {{this.score}}</p>
        <p class="percentage">Percentage: {{this.percentage}}</p>
        <p class="time">Time: {{this.time}}</p>
        <p class="dateCreated">Date: {{this.dateCreated}}</p>
    </div>
</div>
{{/each}}